
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://YOUR_FIREBASE_STUDIO_PREVIEW_URL.cloudworkstations.dev;"
        }
      ]
    }
  ]
}

